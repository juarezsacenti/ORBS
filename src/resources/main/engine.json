{
  "id": "default",
  "description": "Default settings",
  "engineFactory": "br.ufsc.lapesd.sro.example.SROEngine",
  "datasource": {
    "params" : {
      "sourceLocation": "src/resources/test/rating-trainningSet-5%.csv",
      "enclosure": "\"",
      "delimiter": ",",
      "hasHeaderLine": false,
      "hasEventTimestamp": false
    }
  },
  "preparator": {
    "params" : {
    }
  },
  "algorithm":
    {
      "name": "userItemMatrixFC",
      "params": {
        "neighborhoodSize": 100,
        "nativeEvaluatorEnabled": true,
        "seed": 1,
        "iteration": 10,
        "lambda": 0.7,
        "similarItemEvents": ["view"],
        "seenItemEvents": ["view"],
        "unseenOnly": true
      }
    }
}